{
  "diagnostic.warningSign": "⚠️",
  "diagnostic.errorSign": "❌",
  "diagnostic.hintSign": "⚠️",
  "diagnostic.infoSign": "⚠️",
  "diagnostic.enable": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "javascript",
    "_typescript",
    "typescriptreact",
    "json",
    "html",
    "rust",
    "puthon"
  ],
  "eslint.filetypes": ["typescript", "vue", "javascript"],
  "eslint.enable": true,
  "eslint.quiet": true,
  "eslint.autoFixOnSave": false,
  "eslint.run": "onSave",
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.disableSuccessMessage": true,
  "suggest.removeDuplicateItems": true,
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "python.formatting.enabled": "on",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "python -m black"
  "coc.source.emmet.showExpandedAbbreviation": true,
  "coc.source.emmet.showAbbreviationSuggestions": true,
  "coc.source.emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},
}
